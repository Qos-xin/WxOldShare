/* pages/address/index.wxss */

page {
  background: #f5f5f5;
}

.address-wrap {
  padding: 10rpx;
  line-height: 25px;
  background: #fff;
  margin-bottom: 10rpx;
}

.infor-top {
  padding: 10rpx;
}


.btn-wrap {
  border-top: 1px solid #eee;
  padding: 20rpx 10rpx;
  display: flex;
  justify-content: space-between;
  font-size: 13px;
}
.btn-wrap button{
  margin: 0;
  background-color: #1296db;
}
.add-btn {
  position: fixed;
  width: 100%;
  bottom: 0;
  background: #1296db;
  color: #fff;
  text-align: center;
  padding:20rpx;

}
